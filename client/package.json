{
  "name": "gatsby-admin-template",
  "private": true,
  "description": "Admin dashboard template based on Gatsby with @paljs/ui component package",
  "version": "0.1.0",
  "author": "Ahmed Elywa",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "20.0.0",
    "@ckeditor/ckeditor5-react": "2.1.0",
    "@material-ui/core": "^4.10.2",
    "@paljs/icons": "1.0.0",
    "@paljs/ui": "1.0.3",
    "@tinymce/tinymce-react": "3.6.0",
    "gatsby": "2.23.12",
    "gatsby-image": "2.4.9",
    "gatsby-plugin-layout": "1.3.6",
    "gatsby-plugin-manifest": "2.4.14",
    "gatsby-plugin-react-helmet": "3.3.6",
    "gatsby-plugin-sharp": "2.6.14",
    "gatsby-plugin-styled-components": "3.3.6",
    "gatsby-plugin-typescript": "2.4.8",
    "gatsby-source-filesystem": "2.3.14",
    "gatsby-transformer-sharp": "2.5.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "styled-components": "5.1.1"
  },
  "keywords": [
    "react",
    "component",
    "styled",
    "oah"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./public/ && gatsby build",
    "dev": "rm -rf ./public/ && gatsby develop",
